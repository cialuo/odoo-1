<?xml version="1.0" encoding="UTF-8"?>

<templates xml:space="preserve">
    <t t-name="Car">
        <div>
            <div class="popupBox">
                <div class="boxTitle">
                    <div class="closeBox"><a class="close_bt" href="javascript:;"></a></div>
                    <div class="titleInfo infoL">
                        <table>
                            <tr>
                                <td rowspan="2"><span class="licenseNumber fmax">车号：203</span></td>
                                <td><span class="licensePlate">粤K·92823</span></td>
                            </tr>
                            <tr>
                                <td><span class="driver">司机：李素华</span><span class="crew">乘务：张雯</span></td>
                            </tr>
                        </table>
                    </div>
                    <div class="titleInfo infoR">
                        <div class="operation">
                            <div class="scene scene1">
                                <span class="sceneTitle">操作</span>
                                <ul class="operationNav">
                                    <li><a class="back_to_the_field" href="javascript:;">回场</a></li>
                                    <li><a class="start" href="javascript:;">发车</a></li>
                                    <li><a class="sign" href="javascript:;">签到</a></li>
                                </ul>
                            </div>
                            <div class="scene scene1">
                                <span class="sceneTitle">交互</span>
                                <ul class="operationNav">
                                    <li><a class="news" href="javascript:;">消息</a></li>
                                    <li><a class="notice" href="javascript:;">通知</a></li>
                                    <li><a class="conversation" href="javascript:;">通话</a></li>
                                </ul>
                            </div>
                            <a href="javascript:;" class="scene scene2 video">视频</a>
                            <a href="javascript:;" class="scene scene2 police">报警</a>
                            
                        </div>
                        <div class="more">
                            <span class="sceneMore">更多</span>
                            <ul class="morebox">
                                <li><a class="arrivalTime" href="javascript:;">到站时刻</a></li>
                                <li><a class="InformationInteraction" href="javascript:;">信息交互</a></li>
                                <li><a class="geographicalPosition" href="javascript:;">地理位置</a></li>
                                <li><a class="moreInfo" href="javascript:;">更多信息</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="popupContent">
                    <div class="vehicleInformation">
                        <table>
                            <tr>
                                <td>乘客<span class="fmax num">62</span>人</td>
                                <td>满载率<span class="num">90%</span></td>
                                <td>满意度<span class="num">20%</span></td>
                            </tr>
                            <tr>
                                <td>车厢<span class="fmax num">23</span>度</td>
                                <td>室外<span class="num">32</span>度</td>
                                <td>满意度<span class="num">95%</span></td>
                            </tr>
                            <tr>
                                <td>后门<span class="fmax num">关闭</span>人</td>
                                <td>前门<span class="num danger fmax">开启</span></td>
                                <td>当前车速<span class="num">36</span>KM/h</td>
                            </tr>
                            <tr>
                                <td>驶向<span class="fmax num">大亚湾</span>人</td>
                                <td>前车<span class="num">4KM</span></td>
                                <td>后车<span class="num">3KM</span></td>
                            </tr>
                            <tr>
                                <td>回场时间<span class="dangerTime num">12:43</span>人</td>
                                <td>下趟发车<span class="num">12:30</span></td>
                                <td>技术状况<span class="num">良好</span></td>
                            </tr>
                        </table>
                    </div>
                    <div class="carReport">
                        
                    </div>
                </div>
                <div class="lineInfo">
                    <span class="lineRoad">16路</span>
                    第4趟，共10趟
                </div>
            </div>
            <script type="text/javascript">
                $(function(){
                    $('.operation .scene1').hover(function(){
                        $(this).find('.operationNav').show();
                    },function(){
                        $(this).find('.operationNav').hide();
                    });

                    $('.infoR .more').hover(function(){
                        $(this).find('.morebox').show(150);
                    },function(){
                        $(this).find('.morebox').hide();
                    });
                });
            </script>
        </div>
    </t>

    <t t-name="Car_a">
        <div>
            <div class="carInfoScene carzxt" id="carzxt" style="width:344px;height:330px;"></div>
        </div>
    </t>

    <t t-name="Car_video">
        <div>
            <div class="carInfoScene" id="monitor" style="text-align:center;font-size:24px;color:red;">暂时空白（没有监控接口）</div>
        </div>
    </t>

    <t t-name="Car_police">
        <div>
            <div class="carInfoScene policeTemplate" id="police">
                <h1 class="title">报警模式</h1>
                <p class="cont">启动报警模式后，该车会将报警模式前后10分钟及报警模式过程中的全部视频做保留，并上传至视频流媒体服务器。配合后续调查取证</p>
                <a class="onBt" href="javascript:;">启动报警模式</a>
                <ul class="tel">
                    <li>110</li>
                    <li>120</li>
                    <li>119</li>
                </ul>
            </div>
        </div>
    </t>
    
    <t t-name="Car_moreInfo">
        <div>
            <div class="carInfoScene" id="moreInfo" >
                
            </div>
        </div>
    </t>

    <t t-name="Car_moreInfo">
        <div>
            <div class="carInfoScene" id="moreInfo" style="text-align:center;font-size:24px;color:red;">原型图没有给出</div>
        </div>
    </t>

    <t t-name="Car_map">
        <div>
            <div class="carInfoScene" id="carMap" >
            </div>
        </div>
    </t>

    <t t-name="Car_passenge">
        <div>
            <div class="carPassenge" id="carPassengerFlow" >
                <div class="carPassengerLeft carPassenger">
                    <div class="top">
                        <p class="topt"><span class="zm mR10">世界之窗</span><span class="lu mR10">16路</span>客流走势图</p>
                        <a class="fache bt" href="javascript:;">发出越站客车</a>
                    </div>
                    <div class="cont"></div>
                </div>
                <div class="carPassengerRight carPassenger">
                    <div class="top">
                        <p class="topt"><span class="lu mR10">16, 23, 45, 38, 54</span>汇总客流走势图</p>
                    </div>
                    <div class="cont"></div>
                </div>
            </div>
        </div>
    </t>

    <t t-name="Car_passengerMap">
        <div class="mapB">
            <div class="carPassengerMap" id="carPassengerMap" style="width:332px;height:214px;" >
            </div>
            <ul class="mapInfoTxt">
                <li><span class="num red">18</span>滞站候车</li>
                <li><span class="num blue">4</span>0.5h上车</li>
                <li><span class="num gray">14</span>0.5h下车</li>
            </ul>
        </div>
    </t>

    <t t-name="Car_express">
        <div>
            <div class="CarExpress" id="CarExpress" >
                正在开发
            </div>
        </div>
    </t>




</templates>