<odoo>
    <data>
        <record id="vehicle_manage_group_category" model="ir.module.category">
            <field name="name">vehicle manage</field>
            <field name="sequence">18</field>
        </record>
        <!--机务员-->
        <record id="vehicle_usage_user" model="res.groups">
            <field name="name">Vehicle User</field>
            <field name="implied_ids" eval="[(4, ref('fleet.fleet_group_user'))]"/>
            <field name="category_id" ref="vehicle_manage_group_category"/>
        </record>
        <!--机务经理-->
        <record id="vehicle_usage_manager" model="res.groups">
            <field name="name">Vehicle Manager</field>
            <field name="implied_ids" eval="[(4, ref('vehicle_usage_user'))]"/>
            <field name="category_id" ref="vehicle_manage_group_category"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>
    </data>
</odoo>
