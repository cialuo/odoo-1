<odoo>
	<data>

        <record id="station_may_arr_time_topic_tree" model="ir.ui.view">
            <field name="name">station_may_arr_time_topic in records tree</field>
            <field name="model">lty_access_base.station_may_arr_time_topic</field>
            <field name="arch" type="xml">
                <tree create="false">
                    <field name="onboard_id"/>
                    <field name="city_code"/>
                    <field name="plan_id"/>
                    <field name="line_direction"/>
                    <field name="gprs_id"/>
                    <field name="station_id"/>
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="act_station_may_arr_time_topic">
            <field name="name">station_may_arr_time_topic records</field>
            <field name="res_model">lty_access_base.station_may_arr_time_topic</field>
            <field name="view_mode">tree</field>
        </record>

    </data>
</odoo>