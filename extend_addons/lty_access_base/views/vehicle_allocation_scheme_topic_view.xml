<odoo>
	<data>

        <record id="vehicle_allocation_scheme_topic_tree" model="ir.ui.view">
            <field name="name">vehicle_allocation_scheme_topic in records tree</field>
            <field name="model">lty_access_base.vehicle_allocation_scheme_topic</field>
            <field name="arch" type="xml">
                <tree create="false">
                    <field name="city_code"/>
                    <field name="route_id"/>
                    <field name="line_direction"/>
                    <field name="department_id"/>
                    <field name="date_type"/>
                    <field name="number_operating_vehicles"/>
                    <field name="number_motor_vehicle"/>
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="act_vehicle_allocation_scheme_topic">
            <field name="name">vehicle_allocation_scheme_topic records</field>
            <field name="res_model">lty_access_base.vehicle_allocation_scheme_topic</field>
            <field name="view_mode">tree</field>
        </record>

    </data>
</odoo>