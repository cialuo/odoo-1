<odoo>
	<data>

        <record id="punctuality_detention_topic_tree" model="ir.ui.view">
            <field name="name">punctuality_detention_topic in records tree</field>
            <field name="model">lty_access_base.punctuality_detention_topic</field>
            <field name="arch" type="xml">
                <tree create="false">
                    <field name="city_code"/>
                    <field name="route_id"/>
                    <field name="line_direction"/>
                    <field name="time_type"/>
                    <field name="date_type"/>
                    <field name="off_date"/>
                    <field name="off_time"/>
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="act_punctuality_detention_topic">
            <field name="name">punctuality_detention_topic records</field>
            <field name="res_model">lty_access_base.punctuality_detention_topic</field>
            <field name="view_mode">tree</field>
        </record>

    </data>
</odoo>