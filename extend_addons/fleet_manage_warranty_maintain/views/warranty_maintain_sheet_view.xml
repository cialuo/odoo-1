            <field name="name">Into Check</field>
            <field name="type">ir.actions.server</field>
            <field name="model_id" ref="model_fleet_warranty_sheet_item"/>
            <field name="state">code</field>
            <field name="code">
                if context.get('active_model') == 'fleet_warranty_sheet_item' and context.get('active_ids'):
                    action = env['fleet_warranty_sheet_item'].browse(context['active_ids']).action_into_check()
            </field>
        </record>

        <record id="WarrantyItemSheet_into_check" model="ir.values">
            <field name="name">Into Check</field>
            <field name="model_id" ref="model_fleet_warranty_sheet_item"/>
