                            <page string="page instruction_ids">
                                <field name="instruction_ids" readonly="1"> <!-- context="{'form_view_ref': 'fleet_manage_warranty_maintain.item_form_action'}"-->
                                    <tree string="tree instruction_ids">
                                        <field name="sequence"/>
                                        <field name="category_id"/>
                                        <field name="item_id"/>
                                        <field name="maintenance_mode"/>
                                        <field name="operational_manual"/>
                                    </tree>
                                </field>
                            </page>

                            <page string="page return_record_ids">
                                <field name="return_record_ids" readonly="1" > <!--options="{'no_create': True, 'no_open': True}" attrs="{'readonly':[('state','!=','dispatch')]}"  context="{'form_view_ref': 'fleet_manage_warranty_maintain.item_form_action'}" -->
                                    <tree string="tree return_record_ids" create="false">
                                        <field name="maintenance_personnel"/>
                                        <field name="sheet_item"/>
                                        <field name="return_reason"/>
                                        <field name="inspect_user_id"/>
                                        <field name="inspect_return_time"/>
                                    </tree>
                                </field>
                            </page>

                            <page string="page more_info">

                                <group col="2">
                                    <group >
                                        <field name="repair_unit" readonly="1"/>
                                        <field name="fleet" readonly="1"/>
                                        <field name="fill_personnel" readonly="1"/>
                                        <field name="remark" />
                                    </group>
                                    <group >
                                        <field name="repair_workshop" readonly="1"/>
                                        <field name="maintenance_level" readonly="1"/>
                                        <field name="fill_personnel_unit" readonly="1"/>
                                        <field name="maintain_location" readonly="1"/>
                                    </group>
                                </group>
