        <!-- 退回重修 -->
        <record id="fleet_warranty_sheet_inspect_return_form_id" model="ir.ui.view">
            <field name="name">fleet_warranty_sheet_inspect_return.form</field>
            <field name="model">fleet_warranty_sheet_inspect_return</field>
            <field name="arch" type="xml">
                <form string="Confirm Inspect">
                    <group>
                        <field name="return_reason"/>
                    </group>
                    <footer>
                        <button string="Confirm Return" name="inspect_return" type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <act_window id="fleet_warranty_sheet_inspect_return_act_id"
            name="Confirm Return"
