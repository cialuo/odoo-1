<odoo>
    <data>

        <!-- 派工页面 -->
        <record model="ir.ui.view" id="fleet_manage_warranty_maintain.item_form_action">
            <field name="name">MaintainSheetItem.form</field>
            <field name="model">fleet_warranty_sheet_item</field>
            <field name="priority">0</field>
            <field name="arch" type="xml">
                <form string="MaintainRepair Form">
                 <header>
                      <field name="state"  widget="statusbar"/>
                 </header>
                 <sheet>
                    <notebook>
                        <page string="MaintainRepair Jobs" >
                                <field name="manhour_manage_ids">
                                    <tree string="tree manhour_manage_ids">
                                        <field name="sequence"/>
                                        <field name="user_id"/>
                                        <field name="plan_start_time"/>
                                        <field name="plan_end_time"/>
                                        <field name="real_start_time"/>
                                        <field name="real_end_time"/>
                                        <field name="work_time"/>
                                        <field name="percentage_work"/>
                                        <field name="real_work"/>
                                    </tree>
                                </field>
                        </page>
                    </notebook>

                    <group col="2">
                              <group>
                                <field name="work_time"/>
                                <field name="plan_start_time" />
                                <field name="plan_end_time" />
                              </group>
                              <group>
                                <field name="user_id"  />

                                <label for="percentage_work"/>
                                <div class="o_row">
                                  <field name="percentage_work"/><span>%</span>
                                </div>
                              </group>
                    </group>

                    <footer>
                     <button name="dispatch" type="object"
                          string="Dispatch" class="oe_highlight oe_edit_only"/>
                    </footer>

                 </sheet>
                </form>
            </field>
        </record>

        <!-- 保养单 tree -->
        <record model="ir.ui.view" id="fleet_manage_warranty_maintain_sheet_view_tree">
            <field name="name">warranty_maintain_sheet.tree</field>
            <field name="model">fleet_warranty_maintain_sheet</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="plan_id"/>
                    <field name="vehicle_id"/>
                    <field name="vehicle_type"/>
                    <field name="license_plate"/>
                    <field name="fleet"/>
                    <field name="operating_mileage"/>
                    <field name="warranty_category"/>
                    <field name="planned_date"/>
                    <field name="vin"/>
                    <field name="average_daily_kilometer"/>
                    <field name="line"/>
                    <field name="maintain_location"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>



        <!-- 保养单 form -->
        <record model="ir.ui.view" id="fleet_manage_warranty_maintain_sheet_view_form">
            <field name="name">warranty_maintain_sheet.form</field>
            <field name="model">fleet_warranty_maintain_sheet</field>
            <field name="arch" type="xml">
                <form>
                    <header>

                        <button name="action_confirm_effective" states="draft" string="Confirm Effective" type="object" class="oe_highlight o_expense_submit"/> <!-- 确认生效 -->

                        <button name="action_manage_handover_sheet" states="dispatch" string="Manage Handover Sheet" type="object" class="oe_highlight o_expense_submit"/> <!-- 管理交接单 -->

                        <button name="action_batch_dispatch" states="dispatch" string="Batch Dispatch" type="object" class="oe_highlight o_expense_submit"/> <!-- 批量派工 -->

                        <!--<button name="action_draft" type="object"-->
                                <!--string="back to draft"-->
                                <!--states="confirmed,done"/>-->

                        <!--<button name="action_confirm" type="object"-->
                                <!--string="confirm" states="draft"-->
                                <!--class="oe_highlight"/>-->

                        <!--<button name="action_done" type="object"-->
                                <!--string="done" states="confirmed"-->
                                <!--class="oe_highlight"/>-->

                        <field name="state" widget="statusbar"/>
                    </header>

                    <sheet>


                        <div class="oe_title">
	                        <label for="name"/>
							<h1><field name="name" readonly="1"/></h1>
                        </div>

                        <group col="2">
                            <group >
                                <field name="vehicle_id"/>
                                <!--<field name="create_uid" readonly="1" />-->
                                <field name="report_repair_user"/>
                                <field name="warranty_category"/>
                            </group>
                            <group >
                                <field name="vehicle_type"/>
                                <field name="create_date" readonly="1" />
                                <field name="plan_id"/>
                            </group>
                        </group>

                        <notebook>
                            <page string="page item_ids">
                                <field name="item_ids" context="{'form_view_ref': 'fleet_manage_warranty_maintain.item_form_action'}" > <!-- context="{'form_view_ref': 'fleet_manage_warranty_maintain.item_form_action'}" -->
                                    <tree string="tree item_ids">
                                        <field name="sequence"/>
                                        <field name="category_id"/>
                                        <field name="item_id"/>
                                        <field name="maintenance_mode"/>
                                        <field name="maintenance_personnel"/>
                                        <field name="state"/>
                                        <field name="inspection_operation"/>
                                        <field name="reinspection_count"/>
                                    </tree>
                                </field>

                                <!--<field name="item_ids" widget="one2many_list">-->
                                    <!--<tree string="tree item_ids" editable="bottom">-->
                                        <!--<field name="sequence" widget="handle"/>-->
                                        <!--<field name="category_id"/>-->
                                        <!--<field name="item_id"/>-->
                                        <!--<field name="maintenance_method"/>-->
                                        <!--<field name="maintenance_personnel"/>-->
                                        <!--<field name="state"/>-->
                                        <!--<field name="inspection_operation"/>-->
                                        <!--<field name="reinspection_count"/>-->
                                    <!--</tree>-->
                                <!--</field>-->

                            </page>

                            <page string="page manhour_manage_ids">
                                <field name="manhour_manage_ids" widget="one2many_list">
                                    <tree string="tree manhour_manage_ids" editable="bottom">
                                        <field name="sequence"/>
                                        <field name="item_category_id"/>
                                        <field name="item_item_id"/>
                                        <field name="user_id"/>
                                        <field name="plan_start_time"/>
                                        <field name="plan_end_time"/>
                                        <field name="real_start_time"/>
                                        <field name="real_end_time"/>
                                        <field name="work_time"/>
                                        <field name="percentage_work"/>
                                        <field name="self_work"/>
                                        <field name="real_work"/>
                                    </tree>
                                </field>
                            </page>

                            <page string="page available_product_ids">
                                <field name="available_product_ids" widget="one2many_list">
                                    <tree string="tree available_product_ids" editable="bottom">
                                        <field name="sequence"/>
                                        <field name="category_id"/>
                                        <field name="item_id"/>
                                        <field name="product_id"/>
                                        <field name="product_code"/>
                                        <field name="product_name"/>
                                        <field name="max_available_count"/>
                                        <field name="default_avail_count"/>
                                        <field name="stock_count"/>
                                        <field name="available_count"/>
                                        <field name="post_old_get_new"/>
                                        <field name="vehicle_type"/>
                                    </tree>
                                </field>
                            </page>

                            <page string="page instruction_ids">
                                <field name="instruction_ids"> <!-- context="{'form_view_ref': 'fleet_manage_warranty_maintain.item_form_action'}"-->
                                    <tree string="tree instruction_ids">
                                        <field name="sequence"/>
                                        <field name="category_id"/>
                                        <field name="item_id"/>
                                        <field name="maintenance_mode"/>
                                        <field name="operational_manual"/>
                                    </tree>
                                </field>
                            </page>

                            <page string="page more_info">

                                <group col="2">
                                    <group >
                                        <field name="repair_unit"/>
                                        <field name="fleet"/>
                                        <field name="fill_personnel"/>
                                        <field name="remark"/>
                                    </group>
                                    <group >
                                        <field name="repair_workshop"/>
                                        <field name="maintenance_level"/>
                                        <field name="fill_personnel_unit"/>
                                        <field name="maintain_location"/>
                                    </group>
                                </group>
                            </page>



                        </notebook>


                    </sheet>
                </form>
            </field>
        </record>

        <!-- 保养单 action -->
        <record model="ir.actions.act_window" id="fleet_warranty_maintain_sheet_action">
            <field name="name">warranty_maintain_sheet</field>
            <field name="res_model">fleet_warranty_maintain_sheet</field>
            <field name="view_mode">tree,form</field>
			<field name="view_ids" eval="[(5, 0, 0),
				(0, 0, {'view_mode': 'tree', 'view_id': ref('fleet_manage_warranty_maintain_sheet_view_tree')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('fleet_manage_warranty_maintain_sheet_view_form')})]"/>

        </record>










        <!-- 车辆保养管理 menu -->
        <menuitem name="fleet_manage_warranty"
                  id="fleet_manage_warranty_maintain_warranty_menuitemId"
                  parent="fleet_manage_menu.fleet_manage_warranty_maintain"/>

        <!-- 保养单 menu -->
        <menuitem name="warranty_maintain_sheet"
                  id="fleet_warranty_maintain_sheet_menuitemId" sequence="23"
                  parent="fleet_manage_warranty_maintain_warranty_menuitemId"
                  action="fleet_warranty_maintain_sheet_action"/>



        <!-- 保养单项目 tree -->
        <record model="ir.ui.view" id="fleet_warranty_maintain_sheet_view_tree">
            <field name="name">maintain_sheet_item.tree</field>
            <field name="model">fleet_warranty_sheet_item</field>
            <field name="arch" type="xml">
                <tree>
					<field name="sequence"/>
					<field name="category_id"/>
					<field name="item_id"/>
					<field name="maintenance_mode"/>
					<field name="maintenance_personnel"/>
					<field name="state"/>
					<field name="inspection_operation"/>
					<field name="reinspection_count"/>
                </tree>
            </field>
        </record>

		<!-- 保养单项目 action -->
        <record model="ir.actions.act_window" id="fleet_manage_warranty_maintain.maintain_sheet_item_action">
            <field name="name">maintain_sheet_item.action</field>
            <field name="res_model">fleet_warranty_sheet_item</field>
            <field name="view_mode">tree</field>
			<field name="view_ids" eval="[(5, 0, 0),
				(0, 0, {'view_mode': 'tree', 'view_id': ref('fleet_warranty_maintain_sheet_view_tree')})]"/>
        </record>

		<!-- 保养单项目 menu -->
        <!--<menuitem name="maintain_sheet_item"-->
                  <!--id="fleet_manage_warranty_maintain.maintain_sheet_item_menuitemId" sequence="24"-->
                  <!--parent="fleet_manage_warranty_maintain_warranty_menuitemId"-->
                  <!--action="fleet_manage_warranty_maintain.maintain_sheet_item_action"/>-->


        <record model='ir.ui.view' id='wizard_dispatch_form'>
            <field name='name'>wizard_dispatch.form</field>
            <field name='model'>fleet_warranty_wizard_dispatch</field>
            <field name='arch' type='xml'>
                <form string='wizard_dispatch form'>

                    <field name="sheetId" invisible="1"/>

                    <notebook>
                        <page string="Items" >
                                <field name='item_id' domain="[('maintainsheet_id.id', '=', sheetId)]" />
                        </page>
                    </notebook>

                    <notebook>
                        <page string="Users" >
                                <field name='user_id'/>
                        </page>
                    </notebook>


                    <footer>
                        <button name="batch_dispatch_manhour" type="object"
                                string="batch_dispatch_manhour" class="oe_highlight"/>
                        or
                        <button special="cancel" string="Cancel"/>
                    </footer>

                </form>
            </field>
        </record>

        <!--<act_window id='launch_session_wizard'-->
                    <!--name='Add item'-->
                    <!--src_model='fleet_warranty_sheet_item'-->
                    <!--res_model='fleet_warranty_wizard_dispatch'-->
                    <!--view_mode='form'-->
                    <!--target='new'-->
                    <!--key2='client_action_multi'/>-->







    </data>
</odoo>