<odoo>
    <data>

        <!-- 派工页面 -->
        <record model="ir.ui.view" id="fleet_manage_warranty_maintain.item_form_action">
            <field name="name">MaintainSheetItem.form</field>
            <field name="model">fleet_warranty_sheet_item</field>
            <field name="priority">0</field>
            <field name="arch" type="xml">
                <form string="MaintainRepair Form">
                 <!--<header>-->
                      <!--<field name="state"  widget="statusbar"/>-->
                 <!--</header>-->
                 <sheet>
                    <div class="oe_title">
	                    <h1><field name="name" readonly="1"/></h1>
                    </div>
                    <notebook>
                        <page string="MaintainRepair Jobs" >
                                <field name="manhour_manage_ids">
                                    <tree string="tree manhour_manage_ids" create="false" editable="false">
                                        <field name="sequence" widget="handle"/>
                                        <field name="user_id" readonly="1"/>
                                        <field name="plan_start_time" readonly="1"/>
                                        <field name="plan_end_time" readonly="1"/>
                                        <field name="real_start_time" readonly="1"/>
                                        <field name="real_end_time" readonly="1"/>
                                        <field name="work_time" readonly="1"/>
                                        <field name="percentage_work" /> <!--readonly="1"-->
                                        <field name="self_work"/>
                                        <field name="real_work" readonly="1"/>
                                    </tree>
                                </field>
                        </page>
                    </notebook>

                    <group col="2" attrs="{'invisible': [('percentage_work', '=', 0)]}"> <!-- attrs="{'invisible': [('percentage_work', '=', 0)]}"  -->
                              <group>
                                <field name="plan_start_time" />
                                <label for="percentage_work"/>
                                <div class="o_row">
                                  <field name="percentage_work"/><span>%</span>
                                </div>
								<field name="important_product_id" invisible="1"/>
							    <field name="vehicle_id" invisible="1"/>
                              </group>
                              <group>
                                <field name="user_id" options="{'no_create': True, 'no_open': True}" />
                                <field name="plan_end_time" />
                              </group>
                    </group>

                    <group col="2" > <!-- attrs="{'invisible': [('percentage_work', '=', 0)]}"  -->
                              <group>
                                <field name="component_ids" widget="many2many_tags" options="{'no_create_edit': True}" /> <!--  attrs="{'invisible': [('is_important_product', '=', False)],'required': [('is_important_product','=', True)]}" -->
                              </group>
                              <group>
                                  <field name="work_time" readonly="1"/>
                              </group>
                    </group>

                    <footer  attrs="{'invisible': [('percentage_work', '=', 0)]}"> <!-- attrs="{'invisible': [('percentage_work', '=', 0)]}" -->
                     <button name="dispatch" type="object"
                          string="Dispatch" class="oe_highlight oe_edit_only pull-right"/>
                    </footer>

                 </sheet>
                </form>
            </field>
        </record>

        <!-- 保养单 tree -->
        <record model="ir.ui.view" id="fleet_manage_warranty_maintain_sheet_view_tree">
            <field name="name">warranty_maintain_sheet.tree</field>
            <field name="model">fleet_warranty_maintain_sheet</field>
            <field name="arch" type="xml">
                <tree create="false">
                    <field name="name"/>
                    <field name="plan_id"/>
                    <field name="vehicle_id"/>
                    <field name="vehicle_type"/>
                    <field name="license_plate"/>
                    <field name="fleet"/>
                    <field name="operating_mileage"/>
                    <field name="warranty_category"/>
                    <field name="planned_date"/>
                    <field name="vin"/>
                    <field name="average_daily_kilometer"/>
                    <field name="line"/>
                    <field name="maintain_location"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>



