                        </notebook>


                    </sheet>
					<div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="message_ids" widget="mail_thread" options='{"thread_level": 1}'/>
                    </div>
                </form>
            </field>
        </record>

        <!-- 保养单 action -->
        <record model="ir.actions.act_window" id="fleet_warranty_maintain_sheet_action">
            <field name="name">warranty_maintain_sheet</field>
            <field name="res_model">fleet_warranty_maintain_sheet</field>
            <field name="view_mode">tree,form</field>
			<field name="view_ids" eval="[(5, 0, 0),
