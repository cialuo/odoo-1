            </field>
        </record>



		<!-- 检验单 form -->
        <record model="ir.ui.view" id="WarrantyInspectSheet_form_id">
            <field name="name">WarrantyInspectSheet_form</field>
            <field name="model">fleet_warranty_sheet_item</field>
			<field name="priority">1</field>
            <field name="arch" type="xml">
                <form string="WarrantyInspectSheet Form">
                	<header>
                        <!--<button name="action_into_check" type="object"-->
