            target="new"
            view_type="form" />



		<!-- 检验单 tree -->
        <record model="ir.ui.view" id="WarrantyInspectSheet_tree_id">
            <field name="name">WarrantyInspectSheet_tree</field>
            <field name="model">fleet_warranty_sheet_item</field>
            <field name="arch" type="xml">
                <tree string="WarrantyInspectSheet Tree" create="false">
