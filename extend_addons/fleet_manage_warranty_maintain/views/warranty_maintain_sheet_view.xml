                                <!--class="oe_highlight"/> &lt;!&ndash; 报检&ndash;&gt;-->

						<button name="action_check_pass" type="object"
                                string="Check Pass"
								states="check"
                                class="oe_highlight"/> <!-- 检验通过-->

						<button name="%(fleet_warranty_sheet_inspect_return_act_id)d"
								type="action"
                                string="Return To Repair"
								states="check"/> <!-- 退回重修-->

                    	<field name="state"  widget="statusbar" statusbar_visible="maintain,check,complete"/>
                	</header>
					<sheet>
						<div class="oe_title">
	                        <label for="item_name"/>
							<h1><field name="item_name" readonly="1"/></h1>
	                    </div>
						<group col="2">
							<group>
								<field name="category_id" readonly="1"/>
								<field name="item_id" readonly="1"/>
								<field name="maintenance_mode" readonly="1"/>
