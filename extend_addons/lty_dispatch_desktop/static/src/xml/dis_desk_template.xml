<?xml version='1.0' encoding='utf-8' ?>
<template xml:space="preserve">
    <t t-name="dispatch_desktop">
		<t t-if="widget.dis_desk">
			<!--<t t-foreach="widget.dis_desk" t-as="widget.dis_desk">-->
				<div class="dispatch_desktop dragContent" t-attf-style="left:#{widget.dis_desk.oneline.line_show_or_hide.left}px;top:#{widget.dis_desk.oneline.line_show_or_hide.top}px;display:#{widget.dis_desk.oneline.line_show_or_hide.show};z-index:#{widget.dis_desk.oneline.line_show_or_hide.zIndex}">
				<div class="content">
					<!--线路信息展示-->
					<div class="line_info dragArea">
						<div  class="fl line_way">
							<ul>
								<li>
									<t t-esc="widget.dis_desk.oneline.line_info.line"/>
								</li>
								<li>支线1</li>
								<li>支线2</li>
								<li>区间</li>
							</ul>
						</div>
						<div class="bus_info fl">
							<ul>
								<li>配车数量：<t t-esc="widget.dis_desk.oneline.line_info.car_num"/></li>
								<li>挂牌数量：<t t-esc="widget.dis_desk.oneline.line_info.list_num"/></li>
								<li>机动车辆：<t t-esc="widget.dis_desk.oneline.line_info.active_car"/></li>
								<li>维保停运：<t t-esc="widget.dis_desk.oneline.line_info.bad_car"/></li>
								<li>共享机位：<t t-esc="widget.dis_desk.oneline.line_info.share_var"/></li>
								<li>信号在线：<t t-esc="widget.dis_desk.oneline.line_info.online"/></li>
								<li>信号掉线：<t t-esc="widget.dis_desk.oneline.line_info.outline"/></li>
								<li>司机：<t t-esc="widget.dis_desk.oneline.line_info.driver"/></li>
								<li>乘务：<t t-esc="widget.dis_desk.oneline.line_info.crew"/></li>
							</ul>
						</div>
						<div class="line_btn">
							<div class="line_btn_content posR">
								<a class="min" href="javascript:;" title="最小化">-</a>
								<a class="line_edit" href="javascript:;" title="设置">+</a>
							</div>
							<div class="edit_content">
								<div class="ovf">
									<a class="del" href="javascript:;" title="最小化">-</a>
								</div>
								<ul>
									<li>
										<ul class="chs">
											<li>2路</li>
											<li>8路</li>
											<li>34路</li>
										</ul>
									</li>
								</ul>
							</div>
						</div>
					</div>
                    <!--线路路况详情-->
					<div class="traffic ovf">
						<div class="road_distribute">
							<div  class="">
								<div class="line_status ovf">
									<ul class="fl">
										<li t-attf-style="background:#{widget.dis_desk.oneline.plan_feedback[0]};">8</li>
										<li t-attf-style="background:#{widget.dis_desk.oneline.plan_feedback[1]};">12</li>
									</ul>
									<ul  class="fr">
										<li t-attf-style="background:#{widget.dis_desk.oneline.plan_feedback[2]};">24</li>
										<li t-attf-style="background:#{widget.dis_desk.oneline.plan_feedback[3]};">12</li>
										<li t-attf-style="background:#{widget.dis_desk.oneline.plan_feedback[4]};">8</li>
										<li t-attf-style="background:#{widget.dis_desk.oneline.plan_feedback[5]};">65</li>
									</ul>
								</div>
								<div class="actual_traffic ovf">
									<canvas class="canvas_left fl" id="canvas_left" width="26" height="116"></canvas>
									<div>
										<canvas width="1190" height="33" class="can_top"></canvas>
										<div class="traffic_car">
											<div class="content_car_road">
												<t t-foreach="widget.dis_desk.oneline.up_run_car" t-as="live_car">
													<div class="line_car" t-attf-style="left:#{live_car.car_left}px">
														<div class="num_car">
															<span><t t-esc="live_car.num_car"></t></span>
														</div>
														<div class="type_car">
															<span><t t-esc="live_car.type_car"></t></span>
														</div>
													</div>
												</t>
											</div>
											<div class="content_car_road">
												<t t-foreach="widget.dis_desk.oneline.down_run_car" t-as="live_car_right">
													<div class="line_car_right" t-attf-style="left:#{live_car_right.car_left}px">
														<div class="num_car">
															<span><t t-esc="live_car_right.num_car"></t></span>
														</div>
														<div class="type_car">
															<span><t t-esc="live_car_right.type_car"></t></span>
														</div>
													</div>
												</t>
											</div>
										</div>
										<canvas width="1190" height="33" class="can_bottom"></canvas>
									</div>
									<canvas class="canvas_right fl" id="canvas_right" width="26" height="116">

									</canvas>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
            <!--</t>-->
		</t>
		<t t-else="">
			<div>
				<div class="dispatch_desktop  dragContent">
					<div class="content">
					<!--线路信息展示-->
						<div class="line_info dragArea">
							<div class="line_btn">
								<div class="line_btn_content posR">
									<a class="min" href="javascript:;" title="最小化">-</a>
									<a class="line_edit" href="javascript:;" title="设置">+</a>
									<div class="edit_content">
										<div class="ovf">
											<a class="del" href="javascript:;" title="最小化">-</a>
										</div>
										<ul>
											<li>
												<ul class="chs">
													<li>2路</li>
													<li>8路</li>
													<li>34路</li>
												</ul>
											</li>
											<li>
												<ul class="chs">
													<li>资源设置</li>
													<li>样式设置</li>
												</ul>
											</li>
										</ul>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</t>
	</t>

	<t t-name="myConsole">
		<div class="my_console">
			<ul>
				<li class="new_console">
					<div class="div-icon">
						<s class="s-icon"></s>
					</div>
					<div class="div-a">
						<a href="#">调度台资源池</a>
					</div>
				</li>
				<li class="save_console">
					<div class="div-icon">
						<s class="s-icon"></s>
					</div>
					<div class="div-a">
						<a href="#">保存控制台</a>
					</div>
				</li>
				<li class="read_console">
					<div class="div-icon">
						<s class="s-icon"></s>
					</div>
					<div class="div-a">
						<a href="#">配置</a>
					</div>
				</li>
			</ul>
		</div>
	</t>
    <!--上下行图表，客流是否异常-->
	<t t-name="updown_line_table">
		<t t-if="widget.dis_desk">
			<div class="updown_line_table posA dragContent" t-attf-style="left:#{widget.dis_desk.oneline.chart_show_or_hide.left}px;top:#{widget.dis_desk.oneline.chart_show_or_hide.top}px;display:#{widget.dis_desk.oneline.chart_show_or_hide.show};z-index:#{widget.dis_desk.oneline.chart_show_or_hide.zIndex}">
				<div class="updown_drag">
					<div class="updown_title dragArea">
						<div class="line_btn">
							<div class="line_btn_content posR">
								<a class="min" href="javascript:;" title="删除">×</a>
							</div>
						</div>
					</div>
                    <!--替换-->
					<div class="carousel ovf">
						<ul class="carousel_content">
							<li>
								<div class="lunbo_child">
								<!--替换-->
									<div class="absnormal_chart fl">
									</div>
									<ul class="absnormal_operate fl">
										<li>
											<div class="absnormal_type">
												<h4>异常</h4>
												<p>实际客流大于运力供给30%</p>
											</div>
											<div class="absnormal_sug">
												<h4>建议</h4>
												<p>2趟/分钟/20分钟,需补充2台机动车</p>
											</div>
										</li>
										<li class="handleBtn ovf">
											<a>驳回</a>
											<a>校准</a>
											<a>忽略</a>
										</li>
									</ul>
								</div>
							</li>
							<li>
								<div class="lunbo_child">
									<div class="lagstation_chart fl">
									</div>
									<ul class="absnormal_operate fl">
										<li>
											<div class="absnormal_type">
												<h4>异常</h4>
												<p>实际客流大于运力供给30%</p>
											</div>
											<div class="absnormal_sug">
												<h4>建议</h4>
												<p>2趟/分钟/20分钟,需补充2台机动车</p>
											</div>
										</li>
										<li class="handleBtn ovf">
											<a>驳回</a>
											<a>校准</a>
											<a>忽略</a>
										</li>
									</ul>
								</div>
							</li>
						</ul>
					</div>
				</div>
			</div>
		</t>
		<t t-else="">
			<div class="updown_line_table posA dragContent">
				<div class="updown_drag">
					<div class="updown_title dragArea">
						<div class="line_btn">
							<div class="line_btn_content posR">
								<a class="min" href="javascript:;" title="删除">x</a>
							</div>
						</div>
					</div>
				</div>
			</div>
		</t>
	</t>
	<t t-name="config">
		<div>
			<div class="modal fade config" tabindex="-1" role="dialog">
		  <div class="modal-dialog" role="document">
			<div class="modal-content">
			  <div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
				<h4 class="modal-title">配置</h4>
			  </div>
			  <div class="modal-body">
				<div>
					<dl>
						<dt>背景设置</dt>
						<dd>背景色</dd>
						<dd>资源字体颜色设置</dd>
						<dd>资源字体大小设置</dd>
					</dl>
					<dl>
						<dt>资源设置</dt>
						<dd>配车数量</dd>
						<dd>机动车辆</dd>
						<dd>维保停运</dd>
						<dd>共享机动</dd>
						<dd>信号在线</dd>
						<dd>司机</dd>
						<dd>乘务员</dd>
					</dl>
				</div>
			  </div>
			  <div class="modal-footer">
				<button type="button" class="btn btn-primary" data-dismiss="modal">确认</button>
				<button type="button" class="btn btn-default btn_cancel">取消</button>
			  </div>
			</div><!-- /.modal-content -->
		  </div><!-- /.modal-dialog -->
		</div>
		</div>

	</t>
</template>
