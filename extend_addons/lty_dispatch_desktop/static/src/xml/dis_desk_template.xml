	<t t-name="myConsole">
		<div class="my_console">
			<ul>
				<li class="new_console">
					<div class="div-icon">
						<s class="s-icon"></s>
					</div>
					<div class="div-a">
						<a href="#">调度台资源池</a>
					</div>
				</li>
				<li class="save_console">
					<div class="div-icon">
						<s class="s-icon"></s>
					</div>
					<div class="div-a">
						<a href="#">保存控制台</a>
