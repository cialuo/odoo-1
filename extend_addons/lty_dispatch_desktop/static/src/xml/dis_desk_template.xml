<template xml:space="preserve">
	<t t-name="dispatch_desktop_component">
		<div t-attf-controllerId="#{widget.data.component_ids}">

		</div>
	</t>
	<t t-name="desktop_top">
		<div>
			<ul class="desktop_head">
			<li>
				<img src="/lty_dispatch_desktop/static/src/img/lty_logo.png" />
				<div>蓝泰源智能公交调度系统</div>
			</li>
			<li id="nowDateTimeSpan"></li>
			<li>
				<div>
					<img src="/lty_dispatch_desktop/static/src/img/icon_call.png"/>
					<div class="desktop_head_deal">应急电话</div>
				</div>
				<div>
					<img src="/lty_dispatch_desktop/static/src/img/icon_user.png"/>
					<div class="desktop_head_deal">调度员：admin</div>
				</div>
			</li>
		</ul>
		</div>
	</t>
    <t t-name="dispatch_desktop">
		<t t-if="widget.dis_desk">
				<div class="dispatch_desktop dragContent" t-attf-tid="#{widget.dis_desk.oneline.id}" t-attf-style="left:#{widget.dis_desk.oneline.line_show_or_hide.left}px;top:#{widget.dis_desk.oneline.line_show_or_hide.top}px;display:#{widget.dis_desk.oneline.line_show_or_hide.show};z-index:#{widget.dis_desk.oneline.line_show_or_hide.zIndex}">
					<div class="content">
					<!--线路信息展示-->
					<div class="line_info dragArea">
						<div  class="fl line_way">
							<ul>
								<li class="line_line"><t t-esc="widget.dis_desk.oneline.line_info.line"/></li>
								<li class="line_line"><t t-esc="widget.dis_desk.oneline.line_info.line"/></li>
								<li class="aaa">

								</li>
								<li>支线2</li>
								<li>区间</li>
							</ul>
						</div>
						<div class="bus_info fl">
							<ul>
								<li>配车数量：

								</li>
								<li>挂牌数量：<t t-esc="widget.dis_desk.oneline.line_info.list_num"/></li>
								<li>机动车辆：<t t-esc="widget.dis_desk.oneline.line_info.active_car"/></li>
								<li>维保停运：<t t-esc="widget.dis_desk.oneline.line_info.bad_car"/></li>
								<li>共享机位：<t t-esc="widget.dis_desk.oneline.line_info.share_var"/></li>
								<li>信号在线：<t t-esc="widget.dis_desk.oneline.line_info.online"/></li>
								<li>信号掉线：<t t-esc="widget.dis_desk.oneline.line_info.outline"/></li>
								<li>司机：<t t-esc="widget.dis_desk.oneline.line_info.driver"/></li>
								<li>乘务：<t t-esc="widget.dis_desk.oneline.line_info.crew"/></li>
							</ul>
						</div>
						<div class="line_btn">
							<div class="line_btn_content posR">
								<a class="min" href="javascript:;" title="最小化">-</a>
								<a class="line_edit" href="javascript:;" title="设置">+</a>
							</div>
							<div class="edit_content">
								<div class="ovf">
									<a class="del" href="javascript:;" title="最小化">-</a>
								</div>
								<ul>
									<li>
										<ul class="chs">
											<t t-foreach="widget.dis_desk.oneline.line_num" t-as="line_item">
												<li><t t-esc="line_item"></t></li>
											</t>
										</ul>
									</li>
								</ul>
							</div>
						</div>
					</div>
                        <!--线路路况详情-->
					<div class="traffic ovf">
						<div class="road_distribute">
							<div  class="">
								<div class="line_status ovf">
									<ul class="fl">
										<li t-attf-style="background:#{widget.dis_desk.oneline.plan_feedback[0]};">8</li>
										<li t-attf-style="background:#{widget.dis_desk.oneline.plan_feedback[1]};">12</li>
									</ul>
									<ul  class="fr">
										<li t-attf-style="background:#{widget.dis_desk.oneline.plan_feedback[2]};">24</li>
										<li t-attf-style="background:#{widget.dis_desk.oneline.plan_feedback[3]};">12</li>
										<li t-attf-style="background:#{widget.dis_desk.oneline.plan_feedback[4]};">8</li>
										<li t-attf-style="background:#{widget.dis_desk.oneline.plan_feedback[5]};">65</li>
									</ul>
								</div>
								<div class="actual_traffic ovf">
									<canvas class="canvas_left fl" width="26" height="116"></canvas>
									<div>
										<canvas width="1190" height="33" class="can_top"></canvas>
										<div class="traffic_car">
											<div class="content_car_road">
												<t t-foreach="widget.dis_desk.oneline.up_run_car" t-as="live_car">
													<div class="line_car" t-attf-style="left:#{live_car.car_left}px">
														<div class="num_car">
															<span><t t-esc="live_car.num_car"></t></span>
														</div>
														<div class="type_car">
															<span><t t-esc="live_car.type_car"></t></span>
														</div>
													</div>
												</t>
											</div>
											<div class="content_car_road">
												<t t-foreach="widget.dis_desk.oneline.down_run_car" t-as="live_car_right">
													<div class="line_car_right" t-attf-style="left:#{live_car_right.car_left}px">
														<div class="num_car">
															<span><t t-esc="live_car_right.num_car"></t></span>
														</div>
														<div class="type_car">
															<span><t t-esc="live_car_right.type_car"></t></span>
														</div>
													</div>
												</t>
											</div>
										</div>
										<canvas width="1190" height="33" class="can_bottom"></canvas>
									</div>
									<canvas class="canvas_right fl" width="26" height="116">

									</canvas>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</t>
