<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <!--盘点明细增加重要部件列-->
        <record id="component_stock_inventory_view_form" model="ir.ui.view">
            <field name="name">component.stock.inventory.view.form</field>
            <field name="model">stock.inventory</field>
            <field name="inherit_id" ref="stock.view_inventory_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='line_ids']/tree//fields[@name='theoretical_qty']" position="before">
                    <field name="component_ids" widget="many2many_tags"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>