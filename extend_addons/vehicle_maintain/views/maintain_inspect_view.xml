<odoo>
    <data>

        <record id="maintain_inspect_return_view" model="ir.ui.view">
            <field name="name">maintain.inspect.return.form</field>
            <field name="model">maintain.inspect.return</field>
            <field name="arch" type="xml">
                <form string="Confirm Inspect">
                    <group>
                        <field name="return_reason"/>
                    </group>
                    <footer>
                        <button string="Confirm Return" name="inspect_return" type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>


        <act_window id="maintain_inspect_return"
            name="Confirm Return"
            res_model="maintain.inspect.return"
            view_mode="form"
            target="new"
            view_type="form" />

    </data>
</odoo>