<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Sequences for maintain.manage.report -->
        <record id="seq_maintain_report" model="ir.sequence">
            <field name="name">Maintain Report</field>
            <field name="code">maintain.manage.report</field>
            <field name="prefix">BXD</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for maintain.manage.repair -->
        <record id="seq_maintain_repair" model="ir.sequence">
            <field name="name">Maintain Repair</field>
            <field name="code">maintain.manage.repair</field>
            <field name="prefix">WXD</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
        <!--预设额定类型-->
        <record id="work_time_type_a" model="work.time.type">
            <field name="name">A类</field>
            <field name="note">小于9米</field>
        </record>
        <record id="work_time_type_b" model="work.time.type">
            <field name="name">B类</field>
            <field name="note">9-12米</field>
        </record>
        <record id="work_time_type_c" model="work.time.type">
            <field name="name">C类</field>
            <field name="note">大于12米</field>
        </record>
    </data>
</odoo>