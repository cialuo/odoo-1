<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">

        <!--重要部件的部件编码规则-->
        <record id="product_component" model="ir.sequence">
            <field name="name">Product Component</field>
            <field name="code">product_component</field>
            <field name="prefix">BJ%(year)s%(month)s</field>
            <field name="padding">0</field>
            <field eval="1" name="number_increment"/>
        </record>

    </data>
</odoo>