<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="line_site_time_sharing_template">
        <div>
            <div class="operate_title">
                <div class="title">线路分时客流</div>
                <div class="screening">
                    <p class="history_passenger_flow part_block part_hour">
                        历史客流日期范围
                        <select name="history_time" id="">
                            <option value="">前30天</option>
                            <option value="">前60天</option>
                            <option value="">前90天</option>
                            <option value="">前365天</option>
                            <option value="">所有</option>
                        </select>
                    </p>
                    <p class="predict_passenger_flow part_block part_hour">
                        预测客流日期选择
                        <input class="oe_datepicker_input predict_time" type="text"/>
                    </p>

                    <ul class="swt_box">
                        <li class="active" name="when">按时</li>
                        <li name="day">按日</li>
                        <li name="weeks">按周</li>
                        <li name="month">按月</li>
                    </ul>
                </div>
                <div class="screening">
                    <p class="line part_block">
                        线路
                        <select name="line" id="">
                            <option value="">236路</option>
                            <option value="">M231路</option>
                            <option value="">229路</option>
                            <option value="">298路</option>
                        </select>
                        <select class="direction" name="direction" id="">
                            <option value="">上行</option>
                            <option value="">下行</option>
                        </select>
                    </p>
                    <p class="data_type part_block">
                        日期类型
                        <select name="" id="">
                            <option value="">工作日</option>
                            <option value="">周末</option>
                            <option value="">节假日</option>
                            <option value="">list item1</option>
                            <option value="">list item2</option>
                            <option value="">list item3</option>
                        </select>
                    </p>
                    <p class="predict_passenger_flow part_block part_data_scope dis_none">
                        日期范围
                        <input class="start_time oe_datepicker_input" type="text"/>
                        <input class="end_time oe_datepicker_input" type="text"/>
                    </p>
                    <a class="ok_bt" href="javascript:;">确定</a>
                </div>
            </div>
            <div class="operate_cont">
            </div>
        </div>
    </t>
    <t t-name="line_hour_chart_template">
        <div class="line_chart">
            <div style="width: 1040px;height: 380px;" class="chart_passenger_flow"></div>
            <div class="chart_peak">
                <canvas class="chart_peak_canvas" width="750" height="8"></canvas>
                <ul class="chart_peak_canvas_title">
                    <li>高峰
                        <div class="gf st"></div>
                    </li>
                    <li>平峰
                        <div class="pf st"></div>
                    </li>
                    <li>低峰
                        <div class="df st"></div>
                    </li>
                </ul>
            </div>
            <div style="width:1040px;height:280px;" class="chart_satisfaction"></div>
            <div class="chart_satisfaction_bt">
                <label>
                    <input class="set_bt way_1" type="radio" name="way" />候车时长与乘车舒适度</label>
                <label>
                    <input class="set_bt way_2" type="radio" name="way" />乘客满意度</label>
            </div>
        </div>
    </t>

    <t t-name="passenger_flow_chart_template">
        <div class="passenger_flow_chart line_chart">
            <div style="width: 1040px;height: 380px;" class="chart_passenger_flow"></div>
        </div>
    </t>
    <t t-name="site_chart_template">
        <div class="site_chart line_chart">
        </div>
    </t>
</templates>