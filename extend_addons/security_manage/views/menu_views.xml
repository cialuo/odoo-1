<openerp>
    <data>


        <!--二级菜单 安全检查项目 action-->
        <record model="ir.actions.act_window" id="security_manage.action_security_check_items">
            <field name="name">security_manage_security_check_item_window</field>
            <field name="res_model">security_manage.check_item</field>
            <field name="view_mode">tree,form</field>
        </record>

        <!--二级菜单 安全检查表 action-->
        <record model="ir.actions.act_window" id="security_manage.action_security_check_table">
            <field name="name">security_manage_security_check_table_window</field>
            <field name="res_model">security_manage.check_table</field>
            <field name="view_mode">tree,form</field>
        </record>
        <!--二级菜单 档案类别管理 action-->
        <record model="ir.actions.act_window" id="security_manage.action_security_archives_class_manage">
            <field name="name">security_manage_security_archives_class_manage_window</field>
            <field name="res_model">security_manage.cls_manage</field>
            <field name="view_mode">tree,form</field>
        </record>

        <!--顶级菜单 安全管理-->
        <!--<menuitem name="security_manage" id="security_manage.menu_root"/>-->
        <menuitem name="security_manage" id="security_manage_menu_root"/>


        <!--安全检查设置 start-->
        <!--一级菜单 安全检查设置-->
        <menuitem name="security_check_settings" id="security_manage.security_check_settings"
                  parent="security_manage_menu_root"/>
        <!--二级菜单 安全检查项目-->
        <menuitem name="security_check_items" id="security_manage.security_check_items"
                  parent="security_manage.security_check_settings"
                  action="security_manage.action_security_check_items"/>
        <!--二级菜单 安全检查表-->
        <menuitem name="security_check_table" id="security_manage.security_check_table"
                  parent="security_manage.security_check_settings"
                  action="security_manage.action_security_check_table"/>
        <!--二级菜单 档案类别管理-->
        <menuitem name="archives_class_manage" id="security_manage.archives_class_manage"
                  parent="security_manage.security_check_settings"
                  action="security_manage.action_security_archives_class_manage"/>
        <!--安全检查设置 end-->

    </data>
</openerp>