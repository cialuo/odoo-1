            <field name="implied_ids" eval="[(4, ref('base.group_user')), (4, ref('stock.group_stock_user')), (4, ref('fleet.fleet_group_user'))]"/>
        </record>

        <record id="warranty_depot" model="res.groups">
            <field name="name">Depot</field>
            <field name="category_id" ref="manage_warranty_group"/>
            <field name="implied_ids" eval="[(4, ref('warranty_driver'))]"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>


        <record id="warranty_repair" model="res.groups">
            <field name="name">Repair</field>
            <field name="category_id" ref="manage_warranty_group"/>
            <field name="implied_ids" eval="[(4, ref('stock.group_stock_user')), (4, ref('fleet.fleet_group_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>

